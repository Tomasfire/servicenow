							"setTimeout(function () { ",
							"    const req3 = {",
							"        url: base_url + api_url + endpoint, ",
							"        method: \"get\",",
							"        header: {",
							"            'Content-Type': content_type,",
							"            'Accept': accept,",
							"            'Authentication': auth",
							"        }",
							"    }",
							"    pm.sendRequest(req3, function (err, response) {",
							"        var response = response.json()",
							"        pm.variables.set(\"create_ticket_element_id\", pm.variables.get(\"create_ticket_sys_id\"))",
							"        pm.variables.set(\"createticket_sysparm_query\", \"element=comments^ORelement=work_notes\")",
